{
  "name": "@vivocha/bot-sdk",
  "version": "2.7.0-beta6",
  "description": "TypeScript / JavaScript SDK to create Vivocha Bot Agents and Filters",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf dist coverage .nyc_output",
    "clean:all": "npm run clean && rimraf node_modules",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "precommit": "npm run build && npm run cover && npm run check-coverage",
    "commit": "git-cz",
    "check-coverage": "./node_modules/.bin/nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "watch:test": "npm t -- -w",
    "test": "npm run build && tsc -p test && mocha --recursive test/**/*.test.js",
    "test:debug": "npm run build && tsc -p test && mocha --inspect-brk --recursive test/**/*.test.js",
    "cover": "./node_modules/.bin/nyc --reporter=lcov --reporter=text npm t",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vivocha/bot-sdk.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "Vivocha Development Team <dev@vivocha.com> (http://www.vivocha.com/)",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/chai-as-promised": "7.1.0",
    "@types/chai-spies": "0.0.0",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.2",
    "@types/node-wit": "^4.2.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "got": "^8.3.1",
    "husky": "^0.14.3",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "semantic-release": "^12.4.1",
    "typescript": "^2.7.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/debug": "0.0.30",
    "@types/express": "^4.11.1",
    "@vivocha/public-entities": "^6.3.11",
    "arrest": "^7.1.0",
    "node-wit": "^4.3.0",
    "serverless-http": "^1.5.5"
  }
}
